extern controller: [lookup:[int256]:int256, checkWhitelist:[int256]:int256]

data mutex

data controller

def init():
    self.controller = 0x0

def setMutex():
    self.controller.checkWhitelist(msg.sender)
    self.mutex = 1
    return(1)

def unsetMutex():
    self.controller.checkWhitelist(msg.sender)
    self.mutex = 0
    return(1)

def getMutex():
    return(self.mutex)