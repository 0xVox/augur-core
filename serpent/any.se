macro any($a:$asz):
    $i = 0
    $result = 0
    while $i < $asz:
        if $a[$i] != 0:
            $result = 1
            break
        $i += 1
    $result

def test_any():
    a = [0, 0, 0, 0, 0]
    asz = 5
    b = [0, 1, 1, 0, 0]
    bsz = 5
    if not any(a:asz) and any(b:bsz):
        return(1)
    return(0)
