def log(x):
    # binary search for integral part
    imin = 0
    imax = 266
    while(imin < imax):
        twice_n = (imin + imax) / 2

        if(self.exp_halfZ[twice_n] <= x):
            imin = twice_n + 1
        else:
            imax = twice_n

    z = 2^64 - x * 2^64 / self.exp_halfZ[twice_n]

    k = 2
    f = z
    zpow = z
    term = 2^255-1
    while(k < 100 and term > 0):
        zpow = zpow * z / 2^64
        term = zpow / k
        f += term
        k += 1

    return(twice_n * 2^63 - f)

def sqrt(n):
    guess = n/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    guess = (guess + n * 2^64 / guess)/2
    return(guess)

def pow(b, e):
    result = self.exp(e*self.log(b)/2^64)
    return(result)
