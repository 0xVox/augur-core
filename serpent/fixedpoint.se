def exp(x):
    if(x < 0):
        v = -x
    else:
        v = x
    
    twice_n = v/(2^63)

    if(twice_n >= 266):
        if(x < 0):
            return(0)
        else:
            return(2^255-1) # maxval of x > 0
    else:
        f = v&(2^63-1)
        acc = 2^64 + f
        k = 2
        fpow = f
        factorial = 1
        term = 2^255-1
        while(k < 100 and term > 0):
            fpow = fpow * f / 2^64
            factorial *= k
            term = fpow / factorial
            acc += term
            k += 1

        preshift_pt = 1
        postshift_pt = 2^64
        ret = self.exp_halfZ[twice_n] * acc / postshift_pt 
        while(ret < 0 and postshift_pt > 0):
            preshift_pt *= 2
            postshift_pt /= 2
            ret = self.exp_halfZ[twice_n] / preshift_pt * acc / postshift_pt

        if(ret < 0):
            # overflow
            if(x < 0):
                return(0)
            else:
                return(2^255-1)
        else:
            if(x < 0):
                return(2^128/ret)
            else:
                return(ret)

