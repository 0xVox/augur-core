macro any($a, $asz):
    with result = 0:
        with i = 0: 
            while i < $asz:
                if $a[i] != 0:
                    result = 1
                    break
                i += 1
            result

def test_any():
    a = [0, 0, 0, 0, 0]
    b = [0, 1, 1, 0, 0]
    c = [1, 1, 1, 1]
    if not any(a, 5) and any(b, 5) and any(c, 4):
        return(1)
    return(0)
